<div class="container-fluid" style=" align:center;">
	<div class="check-list">
		<ul class="list-group">
			<% @checklistItems.each do |item| %>
				<li class="list-group-item">
					<label class="form-check-label">
						<input class="form-check-input" type="checkbox" value="">
						<%= item %>
					</label>
				</li>
			<% end %>
			<center>
			<button type="submit" style="$hillsborough-action-orange" class="btn btn-default btn-lg btn-primary">Submit</button>
		</ul>
	</div>
</div>
<br><br>

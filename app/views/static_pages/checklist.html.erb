<div class="container-fluid" style="margin:15px 15px 15px 15px; align:center; width=55%">
<div class="card">
	<img class="card-img-top" src="..." alt="Card image cap">
  <div class="card-body">
<div class="check-list">
	<ul class="list-group">
		<% @checklistItems.each do |item| %>
			<li class="list-group-item">
			  <label class="form-check-label">
			    <input class="form-check-input" type="checkbox" value="">
			    <%= item %>
			  </label>
			</li>
  	<% end %>
	</ul>
</div>
</div>
</div>
</div>
<br><br>
